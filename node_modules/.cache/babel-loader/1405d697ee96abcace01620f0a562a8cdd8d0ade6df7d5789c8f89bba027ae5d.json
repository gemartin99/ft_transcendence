{"ast":null,"code":"import Phaser from 'phaser';\nlet cursors;\nexport default {\n  mounted() {\n    const config = {\n      type: Phaser.AUTO,\n      width: 800,\n      height: 600,\n      scene: {\n        preload: this.preload,\n        create: this.create,\n        update: this.update\n      }\n    };\n    this.game = new Phaser.Game(config);\n  },\n  methods: {\n    preload() {\n      this.load.image('paddle', require('@/assets/pala.png'));\n      this.load.image('ball', require('@/assets/pelota.png'));\n    },\n    create() {\n      this.paddleLeft = this.physics.add.sprite(50, this.cameras.main.centerY, 'paddle');\n      this.paddleRight = this.physics.add.sprite(750, this.cameras.main.centerY, 'paddle');\n      this.ball = this.physics.add.sprite(this.cameras.main.centerX, this.cameras.main.centerY, 'ball');\n      cursors = this.input.keyboard.createCursorKeys();\n    },\n    update() {\n      if (cursors.up.isDown) {\n        this.paddleRight.setVelocityY(-160);\n      } else if (cursors.down.isDown) {\n        this.paddleRight.setVelocityY(160);\n      } else {\n        this.paddleRight.setVelocityY(0);\n      }\n    }\n  },\n  beforeUnmount() {\n    this.game.destroy(true);\n  }\n};","map":{"version":3,"names":["Phaser","cursors","mounted","config","type","AUTO","width","height","scene","preload","create","update","game","Game","methods","load","image","require","paddleLeft","physics","add","sprite","cameras","main","centerY","paddleRight","ball","centerX","input","keyboard","createCursorKeys","up","isDown","setVelocityY","down","beforeUnmount","destroy"],"sources":["/Users/gemartin/Desktop/trans/src/views/GameView.vue"],"sourcesContent":["<template>\n  <div ref=\"game\"></div>\n</template>\n\n<script>\nimport Phaser from 'phaser';\n\nlet cursors;\n\nexport default {\n  mounted() {\n    const config = {\n      type: Phaser.AUTO,\n      width: 800,\n      height: 600,\n      scene: {\n        preload: this.preload,\n        create: this.create,\n        u